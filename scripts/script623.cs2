// 623
script623(component $component0, int $int1, int $int2, int $int3)
sound_synth(synth_2266, 1, 0);
if (cc_find($component0, $int1) = ^true) {
	cc_settrans(220);
	cc_setontimer("cc_settrans_temporarily_end(event_com, event_comid, 255, calc(clientclock + 25))");
