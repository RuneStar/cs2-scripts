// 2203
[proc,deadman_map_keypress](int $key0)
switch_int ($key0) {
	case ^key_f3 :
		~deadman_map_enable;
	case ^key_f4 :
		~deadman_area_switch;
	case ^key_f5 :
		~deadman_final_area_1_tele;
	case ^key_f6 :
		~deadman_final_area_2_tele;
	case ^key_f7 :
		~deadman_arenas_tele;
	case default :
		return;
}
