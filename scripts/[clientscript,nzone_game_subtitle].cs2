		$int6 = 0;
	} else if ($int6 = 1) {
		sound_synth(cosmic_star, 1, 0);
	}
	$int6 = calc(50 - $int6);
	if (cc_find($component0, $comsubid1) = ^true) {
		cc_settrans(interpolate(0, $int6, 0, 50, 255));
