// 2608
script2608(int $int0)
if ($int0 = 1) {
	cc_settrans(210);
	cc_setonmouserepeat(null);
	cc_setonmouseleave(null);
	cc_setontimer("script2609(event_com, event_comid, calc(clientclock + 40))");
} else {
	cc_settrans(255);
	cc_setonmouserepeat("cc_settrans(event_com, event_comid, 225, null)");
	cc_setonmouseleave("cc_settrans(event_com, event_comid, 255, null)");
	cc_setontimer(null);
}
