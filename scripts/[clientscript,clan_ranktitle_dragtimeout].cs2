// 4315
[clientscript,clan_ranktitle_dragtimeout](int $clock0, component $component1, int $comsubid2, int $comsubid3, int $comsubid4, component $component5)
if (clientclock >= $clock0 & cc_find($component1, $comsubid2) = ^true) {
	if (.cc_find($component1, $comsubid3) = ^true) {
		.cc_sethide(false);
	}
	if (.cc_find($component1, $comsubid4) = ^true) {
		.cc_sethide(false);
	}
	cc_setontimer(null);
	cc_deleteall($component5);
}
