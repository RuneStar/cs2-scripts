// 4829
[proc,ca_setup_display_hoverdisable](component $component0, int $colour1)
if (cc_find($component0, 0) = ^true) {
	cc_settrans(255);
	cc_setonmouseover(null);
	cc_setonmouseleave(null);
}
if (cc_find($component0, 3) = ^true) {
	cc_settrans(0);
	cc_setgraphic("scrollbar_v2,0");
}
if (cc_find($component0, 4) = ^true) {
	cc_setcolour($colour1);
}
