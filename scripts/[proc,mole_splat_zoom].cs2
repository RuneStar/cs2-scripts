[proc,mole_splat_zoom](component $component0)
def_int $int1 = if_getheight($component0);
if ($int1 < 334) {
	$int1 = 334;
} else if ($int1 > 1200) {
	$int1 = 1200;
}
def_int $int2 = calc(180 - interpolate(0, invpow(($int1 - 334), 2), 0, invpow((1200 - 334), 2), 130));
cc_setmodelangle(0, 0, 462, 0, 0, $int2);
